{
  "$schema": "http://json-schema.org/draft-07/schema",
  "properties": {
    "image": { "const": "explorviz/collaboration-service-jvm:latest" },
    "environment": {
      "type": "array",
      "allOf": [
        {
          "contains": {
            "enum": [
              "EXPLORVIZ_LOG_LVL=DEBUG"
            ]
          }
        }
      ]
    }
  },
  "required": ["image", "environment"]
}
